[run]
source = mpdaf
omit =
   lib/mpdaf/conftest.py
   lib/mpdaf/*setup*
   lib/mpdaf/*tests/*
   lib/mpdaf/version*

[report]
show_missing = false
exclude_lines =
    pragma: no cover
    def __repr__
    def info
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
